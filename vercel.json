{
  "version": 2,
  "builds": [
    {
      "src": "*.{html,js,css}",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/ar", "dest": "/ar-embedded.html" },
    { "src": "/about", "dest": "/about.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
