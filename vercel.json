{
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "main": false,
      "master": false
    }
  },
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/ar-experience", "dest": "/public/ar-experience.html" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}